<Page
    class="page"
    navigatingTo="onNavigatingTo"  
    xmlns="http://schemas.nativescript.org/tns.xsd">

    <ActionBar class="action-bar">
        <Label class="action-bar-title" text="Scan"></Label>
    </ActionBar>

    <GridLayout class="page-content">
        <StackLayout>
            <Button text="SCAN" tap="getScan" />


            <Label id="capacitorLabel" text="{{ capacitorValue }}" textWrap="true" />
        </StackLayout>
    </GridLayout>
</Page>